{
  "$schema": "http://json.schemastore.org/template",
  "author": "Sergey Nazarov",
  "classifications": ["Web", "Microservice"],
  "identity": "TelegramBot.Template",
  "name": "TelegramBot Template",
  "shortName": "telegram-bot-template",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "ProjectName",
  "preferNameDirectory": false,
  "symbols": {
    "projectName": {
      "type": "parameter",
      "isRequired": true,
      "replaces": "ProjectName",
      "fileRename": "ProjectName"
    },
    "projectNameInKebab": {
      "type": "parameter",
      "isRequired": true,
      "replaces": "ProjectNameInKebab"
    },
    "telegramBotToken": {
      "type": "paramater",
      "isRequired": false,
      "replaces": "TelegramBotToken"
    }
  }
}
